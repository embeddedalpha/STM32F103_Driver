#include <stdint.h>
#include "main.h"
#include "CAN.h"

int main(void)
{
    MCU_Clock_Init();
    Delay_Config();
    CAN_Init(CAN_BAUDRATE_1000_KBPS);
    GPIO_Pin_Setup(GPIOC, 13, GEN_PUSH_PULL_OUTPUT);

	for(;;)
	{
		GPIOC -> BSRR |= GPIO_BSRR_BR13;
		CAN_Mailbox1.DLC = 8;
		CAN_Mailbox1.ID_Type = CAN_ID_Extended;
		CAN_Mailbox1.Frame_Type = CAN_Frame_Data;
		CAN_Mailbox1.Standard_ID = 0x001;
		CAN_Mailbox1.data[0] = 0xAA;
		CAN_Mailbox1.data[1] = 0xAA;
		CAN_Mailbox1.data[2] = 0xAA;
		CAN_Mailbox1.data[3] = 0xAA;
		CAN_Mailbox1.data[4] = 0xAA;
		CAN_Mailbox1.data[5] = 0xAA;
		CAN_Mailbox1.data[6] = 0xAA;
		CAN_Mailbox1.data[7] = 0xAA;
		CAN_TX_Mailbox1();
//		Delay_s(1);
		GPIOC -> BSRR |= GPIO_BSRR_BS13;
		Delay_ms(100);
	}
}
