#include <stdint.h>
#include "main.h"
#include "CAN.h"

int main(void)
{
    MCU_Clock_Init();
    Delay_Config();

	for(;;)
	{
		CAN_Mailbox1.DLC = 4;
		CAN_Mailbox1.ID_Type = CAN_ID_Standard;
		CAN_Mailbox1.Standard_ID = 0x101;
		CAN_Mailbox1.data[0] = 0xAA;
		CAN_Mailbox1.data[1] = 0xBB;
		CAN_Mailbox1.data[2] = 0xCC;
		CAN_Mailbox1.data[3] = 0xDD;
		CAN_TX_Mailbox1();
		Delay_ms(500);

	}
}
