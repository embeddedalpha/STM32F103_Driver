#ifndef _GPIO_H_
#define _GPIO_H_

#include "main.h"


#define ANALOG_MODE          0x00
#define FLOATING_INPUT       0x01
#define INPUT_PULL_UP_DOWN   0x02

#define GEN_PURPOSE_OUTPUT_PUSH_PULL         0x00
#define GEN_PURPOSE_OUTPUT_OPEN_DRAIN        0x01
#define ALT_FUNCTION_OUTPUT_PUSH_PULL        0x02
#define ALT_FUNCTION_OUTPUT_OPEN_DRAIN       0x03


void GPIO_Pin_Setup(GPIO_TypeDef *Port, uint8_t pin, uint8_t function, uint8_t alt_function);



#endif
